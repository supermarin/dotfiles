{
  "assistant": {
    "default_model": {
      "provider": "copilot_chat",
      "model": "gpt-4o"
    },
    "version": "2"
  },
  "auto_update": false,
  "autosave": {
    "after_delay": {
      "milliseconds": 2000
    }
  },
  "buffer_font_size": 12,
  "buffer_font_family": "Berkeley Mono",
  "load_direnv": "direct",
  "tabs": {
    "git_status": true
  },
  "terminal": {
    "font_family": "Berkeley Mono",
    "line_height": "standard"
  },
  "telemetry": {
    "metrics": false
  },
  "theme": "Nord",
  "ui_font_size": 11,
  "ui_font_family": "Berkeley Mono",
  "vim_mode": true,
  "languages": {
    "Nix": {
      "language_servers": ["nixd"]
    }
  }
}
