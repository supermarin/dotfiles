{
  "features": {
    "edit_prediction_provider": "zed"
  },
  "formatter": "language_server",
  "ssh_connections": [
    {
      "host": "mufasa",
      "args": [
        "-A"
      ],
      "projects": [
        {
          "paths": [
            "~/code"
          ]
        },
        {
          "paths": [
            "~/code/squale-capital"
          ]
        },
        {
          "paths": [
            "~/code/squale-capital/clenow"
          ]
        }
      ]
    }
  ],
  "auto_update": false,
  "autosave": {
    "after_delay": {
      "milliseconds": 2000
    }
  },
  "buffer_font_size": 14.0,
  "buffer_font_family": "Berkeley Mono",
  "load_direnv": "direct",
  "buffer_line_height": "standard",
  "indent_guides": {
    "enabled": false
  },
  "tabs": {
    "git_status": true
  },
  "terminal": {
    "font_family": "Berkeley Mono",
    "line_height": "standard"
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "theme": "Graphene",
  "ui_font_size": 11,
  "ui_font_family": "Berkeley Mono",
  "vim_mode": true,
  "languages": {
    "Nix": {
      "language_servers": [
        "nixd"
      ]
    }
  }
}
